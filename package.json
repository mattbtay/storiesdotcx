{
  "name": "stories.cx",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "concurrently": "3.5.1",
    "node-sass": "^4.9.2",
    "postcss-cli": "^5.0.1",
    "react-scripts": "^1.1.4"
  },
  "dependencies": {
    "firebase": "^4.10.1",
    "postcss": "^6.0.23",
    "prop-types": "^15.6.0",
    "re-base": "3.2.2",
    "react": "^16.3.0-alpha.1",
    "react-dom": "^16.3.0-alpha.1",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "serve": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, node-sass\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "node-sass --output-style compressed -o src/css src/css",
    "styles:watch": "node-sass --watch --output-style compressed -o src/css src/css"
  }
}
